{
  "name": "appsscript-lib",
  "version": "0.0.1",
  "description": "Bundled Open Source Libaries for Google Apps Script",
  "scripts": {
    "generate:schemaType": "jiti bin/generateSchemaType",
    "build": "tsgo --noEmit && rimraf dist/ && rolldown -c && cpy lib/*/*.json dist/",
    "check": "biome check --write",
    "prepush": "pnpm run check && pnpm run build && pnpm run test",
    "push": "jiti bin/publish",
    "test": "vitest run --coverage"
  },
  "keywords": [],
  "author": "luthpg",
  "license": "MIT",
  "packageManager": "pnpm@10.17.0",
  "dependencies": {
    "diff": "8.0.2",
    "luxon": "^3.7.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@google/clasp": "3.0.6-alpha",
    "@types/luxon": "^3.7.1",
    "@types/node": "^24.5.2",
    "@typescript/native-preview": "7.0.0-dev.20250919.1",
    "@vitest/coverage-v8": "3.2.4",
    "cpy-cli": "^6.0.0",
    "jiti": "^2.5.1",
    "json-schema-to-typescript": "^15.0.4",
    "rimraf": "^6.0.1",
    "rolldown": "1.0.0-beta.38",
    "rolldown-plugin-remove-export": "^0.1.3",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
